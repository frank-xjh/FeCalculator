{
  "name": "FeCalculator",
  "version": "1.0.0",
  "description": "Easy Calculator",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ FeCalculator --all --electron-version=2.0.7 --app-version=1.0.0 --out=./outApp --overwrite --icon=./static/icon.ico",
    "dist": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.frank.FeCalculator",
    "copyright": "FecTok",
    "productName": "FeCalculator",
    "win": {
      "icon": "./static/icon.ico"
    }
  },
