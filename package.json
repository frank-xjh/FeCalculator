    "url": "git@github.com:frank-xjh/FeCalculator.git"
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "mathjs": "^5.1.0"
  },
  "devDependencies": {
    "electron": "^2.0.7",
    "electron-packager": "^12.1.0",
    "electron-builder": "^20.27.1"
  },
  "author": "frank",
  "license": "Apache-2.0"
